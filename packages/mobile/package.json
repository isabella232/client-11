{
	"name": "@wcpos/mobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"test": "jest",
		"ios": "react-native run-ios --simulator=\"iPad 12.1\"",
		"android": "react-native run-android",
		"tsc": "tsc",
		"clean": "shx rm -rf ios/build && rm -rf android/build && rm -rf node_modules && watchman watch-del-all",
		"clean:ios": "shx rm -rf ios/build && xcodebuild clean",
		"clean:android": "shx rm -rf android/build && cd android && ./gradlew clean",
		"studio": "open -a /Applications/Android\\ Studio.app ./android/",
		"xcode": "open ios/wcpos.xcodeproj",
		"postversion": "react-native-version --never-amend"
	},
	"dependencies": {
		"@nozbe/watermelondb": "0.17.0-1",
		"react": "16.13.1",
		"react-native": "0.63.0-rc.1",
		"react-native-gesture-handler": "1.6.1",
		"react-native-linear-gradient": "2.5.6",
		"react-native-reanimated": "1.9.0",
		"react-native-safe-area-context": "3.0.2",
		"react-native-screens": "2.8.0",
		"react-native-svg": "12.1.0",
		"react-native-webview": "10.2.3"
	},
	"devDependencies": {
		"@types/react-native": "0.62.12",
		"metro-react-native-babel-preset": "0.59.0",
		"react-native-svg-transformer": "^0.14.3",
		"react-native-version": "4.0.0"
	}
}
