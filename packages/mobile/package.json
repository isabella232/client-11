{
  "name": "@wcpos/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "ios": "react-native run-ios --simulator=\"iPad 12.1\"",
    "android": "react-native run-android",
    "tsc": "tsc",
    "clean": "shx rm -rf ios/build && rm -rf android/build && rm -rf node_modules && watchman watch-del-all",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "xcode": "open ios/wcpos.xcodeproj",
    "postversion": "react-native-version --never-amend"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.16.0",
    "react": "16.13.1",
    "react-native": "0.62.2",
    "react-native-gesture-handler": "1.6.1",
    "react-native-reanimated": "1.8.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "2.4.0",
    "react-native-webview": "9.1.4"
  },
  "devDependencies": {
    "@types/react-native": "0.62.2",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native-version": "4.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
