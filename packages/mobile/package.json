{
  "name": "@wcpos/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "ios": "react-native run-ios --simulator=\"iPad 12.1\"",
    "android": "react-native run-android",
    "tsc": "tsc",
    "clean": "shx rm -rf ios/build && rm -rf android/build && rm -rf node_modules && watchman watch-del-all",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "xcode": "open ios/wcpos.xcodeproj",
    "postversion": "react-native-version --never-amend"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.12.3",
    "react-native": "0.59.5",
    "react-native-gesture-handler": "1.2.1",
    "react-native-splash-screen": "3.2.0"
  },
  "devDependencies": {
    "@types/react-native": "0.57.51",
    "metro-react-native-babel-preset": "0.54.0",
    "react-native-version": "3.1.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
