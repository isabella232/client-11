{
	"name": "wcpos",
	"version": "0.0.1",
	"main": "packages/web/src/index.tsx",
	"repository": "https://github.com/wcpos/client",
	"author": "kilbot",
	"license": "MIT",
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		],
		"nohoist": []
	},
	"scripts": {
		"desktop": "yarn --cwd packages/desktop",
		"mobile": "yarn --cwd packages/mobile",
		"storybook": "yarn --cwd packages/storybook",
		"web": "yarn --cwd packages/web",
		"common": "yarn --cwd packages/common",
		"clean": "yarn workspaces run clean && yarn cache clean && shx rm -rf yarn.lock && rm -rf node_modules",
		"version": "yarn workspaces run version --no-git-tag-version --new-version $npm_package_version && yarn workspace @wcpos/mobile postversion",
		"postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\"",
		"postinstall": "cd packages/mobile/ios && pod install",
		"jest": "jest --coverage --silent",
		"test": "yarn jest"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "7.10.4",
		"@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
		"@babel/plugin-proposal-optional-chaining": "7.10.4",
		"@types/jest": "26.0.3",
		"@types/rewire": "2.5.28",
		"@typescript-eslint/eslint-plugin": "3.5.0",
		"@typescript-eslint/parser": "3.5.0",
		"babel-eslint": "10.1.0",
		"babel-jest": "26.1.0",
		"eslint": "7.4.0",
		"eslint-config-airbnb": "18.2.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jest": "^23.17.1",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.20.3",
		"eslint-plugin-react-hooks": "4.0.5",
		"eslint-plugin-react-native": "3.8.1",
		"jest": "26.1.0",
		"postinstall-postinstall": "2.1.0",
		"prettier": "2.0.5",
		"shx": "0.3.2"
	},
	"dependencies": {}
}
