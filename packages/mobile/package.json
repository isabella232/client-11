{
	"name": "@wcpos/mobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "env-cmd react-native start",
		"test": "jest",
		"ios": "env-cmd yarn pod:install && react-native run-ios --simulator=\"iPad 12.1\"",
		"android": "react-native run-android",
		"tsc": "tsc",
		"clean": "shx rm -rf ios/build && rm -rf android/build && rm -rf node_modules && watchman watch-del-all",
		"clean:ios": "shx rm -rf ios/build && xcodebuild clean",
		"clean:android": "shx rm -rf android/app/build && cd android && ./gradlew clean",
		"studio": "open -a /Applications/Android\\ Studio.app ./android/",
		"xcode": "open ios/wcpos.xcodeproj",
		"postversion": "react-native-version --never-amend",
		"analyze:android": "react-native-bundle-visualizer --platform android --entry-file ./index.js --reset-cache",
		"analyze:ios": "react-native-bundle-visualizer --platform ios --entry-file ./index.js --reset-cache",
		"pod:install": "cd ios/ && pod install --verbose --no-repo-update",
		"build:android:release": "cd android && ./gradlew assembleRelease"
	},
	"dependencies": {
		"@react-native-community/netinfo": "5.9.5",
		"@sentry/react-native": "1.6.3",
		"base-64": "0.1.0",
		"events": "3.1.0",
		"pouchdb-adapter-react-native-sqlite": "2.0.0",
		"react-native": "0.63.1",
		"react-native-device-info": "5.6.2",
		"react-native-gesture-handler": "1.6.1",
		"react-native-linear-gradient": "2.5.6",
		"react-native-reanimated": "1.9.0",
		"react-native-safe-area-context": "3.1.1",
		"react-native-screens": "2.9.0",
		"react-native-sqlite-2": "3.0.1",
		"react-native-svg": "12.1.0",
		"react-native-webview": "10.3.2",
		"rxdb": "9.3.0"
	},
	"devDependencies": {
		"@types/react-native": "0.63.1",
		"babel-plugin-transform-inline-environment-variables": "0.4.3",
		"env-cmd": "10.1.0",
		"metro-react-native-babel-preset": "0.60.0",
		"react-native-bundle-visualizer": "^2.2.1",
		"react-native-svg-transformer": "^0.14.3",
		"react-native-version": "4.0.0"
	}
}
